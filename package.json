{
  "name": "sse-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:iframe": "vite -c vite.config.iframe.js",
    "dev:app": "vite -c vite.config.app.js",
    "dev": "set VITE_RUN_SINGLE=false && concurrently -k \"pnpm run dev:*\"",
    "build:iframe": "vite build -c vite.config.iframe.js",
    "build:app": "vite build -c vite.config.app.js",
    "build": "pnpm run build:app && pnpm run build:iframe",
    "preview:iframe": "node --experimental-modules ./staging/server.mjs ",
    "preview:app": "vite preview -c vite.config.app.js  ",
    "preview": "pnpm run build && concurrently -k \"pnpm run preview:*\""
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "concurrently": "^8.2.2",
    "sass": "^1.69.5",
    "vite": "^4.4.11",
    "vite-plugin-css-injected-by-js": "^3.3.0"
  }
}
